{
  "expo": {
    "name": "RocketDeliveryMobileMOD14Ric",
    "slug": "RocketDeliveryMobileMOD14Ric",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/Images/AppIcon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/Images/AppLogoV2.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*", "assets/fonts/*", "assets/fonts/Oswald/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/Images/AppIcon.png"
    },
    "plugins": [
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/arial.ttf",
            "./assets/fonts/arialbd.ttf",
            "./assets/fonts/arialbi.ttf",
            "./assets/fonts/ariali.ttf",
            "./assets/fonts/Oswald-Regular.ttf",
            "./assets/fonts/Oswald-Bold.ttf",
            "./assets/fonts/Oswald-ExtraLight.ttf",
            "./assets/fonts/Oswald-Light.ttf",
            "./assets/fonts/Oswald-Medium.ttf",
            "./assets/fonts/Oswald-SemiBold.ttf"
          ]
        }
      ]
    ]
  }
}
